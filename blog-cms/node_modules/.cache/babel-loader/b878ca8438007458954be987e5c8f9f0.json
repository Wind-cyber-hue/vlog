A,SAAA;AAIA,OAnBA;AAoBA,KAhEA;AAiEA,IAAA,aAjEA,yBAiEA,KAjEA,EAiEA;AAAA;;AACA,MAAA,cAAA,CAAA,KAAA,CAAA,CAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,YAAA,GAAA,CAAA,IAAA,KAAA,GAAA,EAAA;AACA,UAAA,MAAA,CAAA,UAAA,CAAA,GAAA,CAAA,GAAA;;AACA,UAAA,MAAA,CAAA,OAAA;AACA,SAHA,MAGA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,GAAA,CAAA,GAAA;AACA;AACA,OAPA,EAOA,KAPA,CAOA,YAAA;AACA,QAAA,MAAA,CAAA,QAAA,CAAA,MAAA;AACA,OATA;AAUA,KA5EA;AA6EA,IAAA,cA7EA,0BA6EA,GA7EA,EA6EA;AACA,WAAA,QAAA,qBAAA,GAAA;AACA,WAAA,iBAAA,GAAA,IAAA;AACA,KAhFA;AAiFA,IAAA,eAjFA,6BAiFA;AACA,WAAA,KAAA,CAAA,UAAA,CAAA,WAAA;AACA,KAnFA;AAoFA,IAAA,gBApFA,8BAoFA;AACA,WAAA,QAAA,GAAA,EAAA;AACA,WAAA,KAAA,CAAA,WAAA,CAAA,WAAA;AACA,KAvFA;AAwFA,IAAA,MAxFA,oBAwFA;AAAA;;AACA,WAAA,KAAA,CAAA,UAAA,CAAA,QAAA,CAAA,UAAA,KAAA,EAAA;AACA,YAAA,KAAA,EAAA;AACA,UAAA,OAAA,CAAA,MAAA,CAAA,OAAA,CAAA,CAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,gBAAA,GAAA,CAAA,IAAA,KAAA,GAAA,EAAA;AACA,cAAA,MAAA,CAAA,UAAA,CAAA,GAAA,CAAA,GAAA;;AACA,cAAA,MAAA,CAAA,gBAAA,GAAA,KAAA;;AACA,cAAA,MAAA,CAAA,OAAA;AACA,aAJA,MAIA;AACA,cAAA,MAAA,CAAA,QAAA,CAAA,GAAA,CAAA,GAAA;AACA;AACA,WARA,EAQA,KARA,CAQA,YAAA;AACA,YAAA,MAAA,CAAA,QAAA,CAAA,MAAA;AACA,WAVA;AAWA;AACA,OAdA;AAeA,KAxGA;AAyGA,IAAA,OAzGA,qBAyGA;AAAA;;AACA,WAAA,KAAA,CAAA,WAAA,CAAA,QAAA,CAAA,UAAA,KAAA,EAAA;AACA,YAAA,KAAA,EAAA;AACA,UAAA,QAAA,CAAA,MAAA,CAAA,QAAA,CAAA,CAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,gBAAA,GAAA,CAAA,IAAA,KAAA,GAAA,EAAA;AACA,cAAA,MAAA,CAAA,UAAA,CAAA,GAAA,CAAA,GAAA;;AACA,cAAA,MAAA,CAAA,iBAAA,GAAA,KAAA;;AACA,cAAA,MAAA,CAAA,OAAA;AACA,aAJA,MAIA;AACA,cAAA,MAAA,CAAA,QAAA,CAAA,GAAA,CAAA,GAAA;AACA;AACA,WARA,EAQA,KARA,CAQA,YAAA;AACA,YAAA,MAAA,CAAA,QAAA,CAAA,MAAA;AACA,WAVA;AAWA;AACA,OAdA;AAeA,KAzHA;AA0HA,IAAA,SA1HA,uBA0HA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,YAAA;AACA;AA5HA;AA/BA,CAAA","sourcesContent":["<template>\n\t<div>\n\t\t<!--面包屑导航-->\n\t\t<Breadcrumb parentTitle=\"系统管理\"/>\n\n\t\t<el-form inline>\n\t\t\t<el-form-item>\n\t\t\t\t<el-button type=\"primary\" size=\"mini\" icon=\"el-icon-plus\" @click=\"addDialogVisible=true\">添加</el-button>\n\t\t\t</el-form-item>\n\t\t\t<el-form-item>\n\t\t\t\t<el-button type=\"warning\" size=\"mini\" icon=\"el-icon-document-checked\" @click=\"goLogPage\">日志</el-button>\n\t\t\t</el-form-item>\n\t\t</el-form>\n\n\t\t<el-table :data=\"jobList\">\n\t\t\t<el-table-column label=\"任务id\" prop=\"jobId\" width=\"80\"></el-table-column>\n\t\t\t<el-table-column label=\"bean\" prop=\"beanName\"></el-table-column>\n\t\t\t<el-table-column label=\"方法名\" prop=\"methodName\"></el-table-column>\n\t\t\t<el-table-column label=\"参数\" prop=\"params\"></el-table-column>\n\t\t\t<el-table-column label=\"cron\" prop=\"cron\"></el-table-column>\n\t\t\t<el-table-column label=\"状态\" width=\"80\">\n\t\t\t\t<template v-slot=\"scope\">\n\t\t\t\t\t<el-switch v-model=\"scope.row.status\" @change=\"jobStatusChanged(scope.row)\"></el-switch>\n\t\t\t\t</template>\n\t\t\t</el-table-column>\n\t\t\t<el-table-column label=\"备注\" prop=\"remark\"></el-table-column>\n\t\t\t<el-table-column label=\"创建时间\" width=\"170\">\n\t\t\t\t<template v-slot=\"scope\">{{ scope.row.createTime | dateFormat }}</template>\n\t\t\t</el-table-column>\n\t\t\t<el-table-column label=\"操作\" width=\"290\">\n\t\t\t\t<template v-slot=\"scope\">\n\t\t\t\t\t<el-button type=\"warning\" icon=\"el-icon-caret-right\" size=\"mini\" @click=\"runOnce(scope.row.jobId)\">执行一次</el-button>\n\t\t\t\t\t<el-button type=\"primary\" icon=\"el-icon-edit\" size=\"mini\" @click=\"showEditDialog(scope.row)\">编辑</el-button>\n\t\t\t\t\t<el-popconfirm title=\"确定删除吗？\" icon=\"el-icon-delete\" iconColor=\"red\" @onConfirm=\"deleteJobById(scope.row.jobId)\">\n\t\t\t\t\t\t<el-button size=\"mini\" type=\"danger\" icon=\"el-icon-delete\" slot=\"reference\">删除</el-button>\n\t\t\t\t\t</el-popconfirm>\n\t\t\t\t</template>\n\t\t\t</el-table-column>\n\t\t</el-table>\n\n\t\t<!--分页-->\n\t\t<el-pagination @size-change=\"handleSizeChange\" @current-change=\"handleCurrentChange\" :current-page=\"queryInfo.pageNum\"\n\t\t               :page-sizes=\"[10, 20, 30, 50]\" :page-size=\"queryInfo.pageSize\" :total=\"total\"\n\t\t               layout=\"total, sizes, prev, pager, next, jumper\" background>\n\t\t</el-pagination>\n\n\t\t<!--添加任务对话框-->\n\t\t<el-dialog title=\"添加任务\" width=\"50%\" :visible.sync=\"addDialogVisible\" :close-on-click-modal=\"false\" @close=\"addDialogClosed\">\n\t\t\t<!--内容主体-->\n\t\t\t<el-form :model=\"addForm\" :rules=\"formRules\" ref=\"addFormRef\" label-width=\"80px\">\n\t\t\t\t<el-form-item label=\"bean\" prop=\"beanName\">\n\t\t\t\t\t<el-input v-model=\"addForm.beanName\"></el-input>\n\t\t\t\t</el-form-item>\n\t\t\t\t<el-form-item label=\"方法名\" prop=\"methodName\">\n\t\t\t\t\t<el-input v-model=\"addForm.methodName\"></el-input>\n\t\t\t\t</el-form-item>\n\t\t\t\t<el-form-item label=\"参数\" prop=\"params\">\n\t\t\t\t\t<el-input v-model=\"addForm.params\"></el-input>\n\t\t\t\t</el-form-item>\n\t\t\t\t<el-form-item label=\"cron\" prop=\"cron\">\n\t\t\t\t\t<el-input v-model=\"addForm.cron\"></el-input>\n\t\t\t\t</el-form-item>\n\t\t\t\t<el-form-item label=\"备注\" prop=\"remark\">\n\t\t\t\t\t<el-input v-model=\"addForm.remark\"></el-input>\n\t\t\t\t</el-form-item>\n\t\t\t</el-form>\n\t\t\t<!--底部-->\n\t\t\t<span slot=\"footer\">\n\t\t\t\t<el-button @click=\"addDialogVisible=false\">取 消</el-button>\n\t\t\t\t<el-button type=\"primary\" @click=\"addJob\">确 定</el-button>\n\t\t\t</span>\n\t\t</el-dialog>\n\n\t\t<!--编辑任务对话框-->\n\t\t<el-dialog title=\"编辑任务\" width=\"50%\" :visible.sync=\"editDialogVisible\" :close-on-click-modal=\"false\" @close=\"editDialogClosed\">\n\t\t\t<!--内容主体-->\n\t\t\t<el-form :model=\"editForm\" :rules=\"formRules\" ref=\"editFormRef\" label-width=\"80px\">\n\t\t\t\t<el-form-item label=\"bean\" prop=\"beanName\">\n\t\t\t\t\t<el-input v-model=\"editForm.beanName\"></el-input>\n\t\t\t\t</el-form-item>\n\t\t\t\t<el-form-item label=\"方法名\" prop=\"methodName\">\n\t\t\t\t\t<el-input v-model=\"editForm.methodName\"></el-input>\n\t\t\t\t</el-form-item>\n\t\t\t\t<el-form-item label=\"参数\" prop=\"params\">\n\t\t\t\t\t<el-input v-model=\"editForm.params\"></el-input>\n\t\t\t\t</el-form-item>\n\t\t\t\t<el-form-item label=\"cron\" prop=\"cron\">\n\t\t\t\t\t<el-input v-model=\"editForm.cron\"></el-input>\n\t\t\t\t</el-form-item>\n\t\t\t\t<el-form-item label=\"备注\" prop=\"remark\">\n\t\t\t\t\t<el-input v-model=\"editForm.remark\"></el-input>\n\t\t\t\t</el-form-item>\n\t\t\t</el-form>\n\t\t\t<!--底部-->\n\t\t\t<span slot=\"footer\">\n\t\t\t\t<el-button @click=\"editDialogVisible=false\">取 消</el-button>\n\t\t\t\t<el-button type=\"primary\" @click=\"editJob\">确 定</el-button>\n\t\t\t</span>\n\t\t</el-dialog>\n\t</div>\n</template>\n\n<script>\n\timport Breadcrumb from \"@/components/Breadcrumb\";\n\timport {getJobList, updateJobStatus, runJobOnce, deleteJobById, addJob, editJob} from \"@/api/schedule\";\n\n\texport default {\n\t\tname: \"ScheduleJobList\",\n\t\tcomponents: {Breadcrumb},\n\t\tdata() {\n\t\t\treturn {\n\t\t\t\tqueryInfo: {\n\t\t\t\t\tpageNum: 1,\n\t\t\t\t\tpageSize: 10\n\t\t\t\t},\n\t\t\t\tjobList: [],\n\t\t\t\ttotal: 0,\n\t\t\t\taddDialogVisible: false,\n\t\t\t\teditDialogVisible: false,\n\t\t\t\taddForm: {\n\t\t\t\t\tbeanName: '',\n\t\t\t\t\tmethodName: '',\n\t\t\t\t\tparams: '',\n\t\t\t\t\tcron: '',\n\t\t\t\t\tremark: ''\n\t\t\t\t},\n\t\t\t\teditForm: {},\n\t\t\t\tformRules: {\n\t\t\t\t\tbeanName: [{required: true, message: '请输入bean名称', trigger: 'blur'}],\n\t\t\t\t\tmethodName: [{required: true, message: '请输入方法名', trigger: 'blur'}],\n\t\t\t\t\tcron: [{required: true, message: '请输入cron表达式', trigger: 'blur'}],\n\t\t\t\t}\n\t\t\t}\n\t\t},\n\t\tcreated() {\n\t\t\tthis.getData()\n\t\t},\n\t\tmethods: {\n\t\t\tgetData() {\n\t\t\t\tgetJobList(this.queryInfo).then(res => {\n\t\t\t\t\tif (res.code === 200) {\n\t\t\t\t\t\tthis.msgSuccess(res.msg)\n\t\t\t\t\t\tthis.jobList = res.data.list\n\t\t\t\t\t\tthis.total = res.data.total\n\t\t\t\t\t} else {\n\t\t\t\t\t\tthis.msgError(res.msg)\n\t\t\t\t\t}\n\t\t\t\t}).catch(() => {\n\t\t\t\t\tthis.msgError(\"请求失败\")\n\t\t\t\t})\n\t\t\t},\n\t\t\thandleSizeChange(newSize) {\n\t\t\t\tthis.queryInfo.pageSize = newSize\n\t\t\t\tthis.getData()\n\t\t\t},\n\t\t\thandleCurrentChange(newPage) {\n\t\t\t\tthis.queryInfo.pageNum = newPage\n\t\t\t\tthis.getData()\n\t\t\t},\n\t\t\tjobStatusChanged(row) {\n\t\t\t\tlet text = row.status ? '启用' : '暂停'\n\t\t\t\tthis.$confirm(`确认要<strong style=\"color: red\">${text}</strong>该任务吗?`, '提示', {\n\t\t\t\t\tconfirmButtonText: '确定',\n\t\t\t\t\tcancelButtonText: '取消',\n\t\t\t\t\ttype: 'warning',\n\t\t\t\t\tdangerouslyUseHTMLString: true\n\t\t\t\t}).then(() => {\n\t\t\t\t\tupdateJobStatus(row.jobId, row.status).then(res => {\n\t\t\t\t\t\tif (res.code === 200) {\n\t\t\t\t\t\t\tthis.msgSuccess(res.msg)\n\t\t\t\t\t\t} else {\n\t\t\t\t\t\t\tthis.msgError(res.msg)\n\t\t\t\t\t\t}\n\t\t\t\t\t}).catch(() => {\n\t\t\t\t\t\tthis.msgError(\"请求失败\")\n\t\t\t\t\t})\n\t\t\t\t}).catch(() => {\n\t\t\t\t\trow.status = !row.status\n\t\t\t\t})\n\t\t\t},\n\t\t\trunOnce(jobId) {\n\t\t\t\tthis.$confirm('确认要立即执行一次该任务吗?', '提示', {\n\t\t\t\t\tconfirmButtonText: '确定',\n\t\t\t\t\tcancelButtonText: '取消',\n\t\t\t\t\ttype: 'warning'\n\t\t\t\t}).then(() => {\n\t\t\t\t\trunJobOnce(jobId).then(res => {\n\t\t\t\t\t\tif (res.code === 200) {\n\t\t\t\t\t\t\tthis.msgSuccess(res.msg)\n\t\t\t\t\t\t} else {\n\t\t\t\t\t\t\tthis.msgError(res.msg)\n\t\t\t\t\t\t}\n\t\t\t\t\t}).catch(() => {\n\t\t\t\t\t\tthis.msgError(\"请求失败\")\n\t\t\t\t\t})\n\t\t\t\t}).catch(() => {\n\t\t\t\t\tthis.$message({\n\t\t\t\t\t\ttype: 'info',\n\t\t\t\t\t\tmessage: '已取消执行'\n\t\t\t\t\t})\n\t\t\t\t})\n\t\t\t},\n\t\t\tdeleteJobById(jobId) {\n\t\t\t\tdeleteJobById(jobId).then(res => {\n\t\t\t\t\tif (res.code === 200) {\n\t\t\t\t\t\tthis.msgSuccess(res.msg)\n\t\t\t\t\t\tthis.getData()\n\t\t\t\t\t} else {\n\t\t\t\t\t\tthis.msgError(res.msg)\n\t\t\t\t\t}\n\t\t\t\t}).catch(() => {\n\t\t\t\t\tthis.msgError(\"请求失败\")\n\t\t\t\t})\n\t\t\t},\n\t\t\tshowEditDialog(row) {\n\t\t\t\tthis.editForm = {...row}\n\t\t\t\tthis.editDialogVisible = true\n\t\t\t},\n\t\t\taddDialogClosed() {\n\t\t\t\tthis.$refs.addFormRef.resetFields()\n\t\t\t},\n\t\t\teditDialogClosed() {\n\t\t\t\tthis.editForm = {}\n\t\t\t\tthis.$refs.editFormRef.resetFields()\n\t\t\t},\n\t\t\taddJob() {\n\t\t\t\tthis.$refs.addFormRef.validate(valid => {\n\t\t\t\t\tif (valid) {\n\t\t\t\t\t\taddJob(this.addForm).then(res => {\n\t\t\t\t\t\t\tif (res.code === 200) {\n\t\t\t\t\t\t\t\tthis.msgSuccess(res.msg)\n\t\t\t\t\t\t\t\tthis.addDialogVisible = false\n\t\t\t\t\t\t\t\tthis.getData()\n\t\t\t\t\t\t\t} else {\n\t\t\t\t\t\t\t\tthis.msgError(res.msg)\n\t\t\t\t\t\t\t}\n\t\t\t\t\t\t}).catch(() => {\n\t\t\t\t\t\t\tthis.msgError(\"请求失败\")\n\t\t\t\t\t\t})\n\t\t\t\t\t}\n\t\t\t\t})\n\t\t\t},\n\t\t\teditJob() {\n\t\t\t\tthis.$refs.editFormRef.validate(valid => {\n\t\t\t\t\tif (valid) {\n\t\t\t\t\t\teditJob(this.editForm).then(res => {\n\t\t\t\t\t\t\tif (res.code === 200) {\n\t\t\t\t\t\t\t\tthis.msgSuccess(res.msg)\n\t\t\t\t\t\t\t\tthis.editDialogVisible = false\n\t\t\t\t\t\t\t\tthis.getData()\n\t\t\t\t\t\t\t} else {\n\t\t\t\t\t\t\t\tthis.msgError(res.msg)\n\t\t\t\t\t\t\t}\n\t\t\t\t\t\t}).catch(() => {\n\t\t\t\t\t\t\tthis.msgError(\"请求失败\")\n\t\t\t\t\t\t})\n\t\t\t\t\t}\n\t\t\t\t})\n\t\t\t},\n\t\t\tgoLogPage() {\n\t\t\t\tthis.$router.push('/jobs/logs')\n\t\t\t}\n\t\t}\n\t}\n</script>\n\n<style scoped>\n\t.el-button + span {\n\t\tmargin-left: 10px;\n\t}\n\n\t.el-form--inline .el-form-item {\n\t\tmargin-bottom: 0;\n\t}\n</style>"],"sourceRoot":"src/views/system"}]}